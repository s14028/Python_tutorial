<script type="text/javascript">
    //
    window['recaptcha_rendered'] = false;
    function recaptcha_render() {
        //
        // recaptcha render function
        //
        var recaptcha_container = document.getElementById('recaptcha-container');
        if(recaptcha_container.className.indexOf('hidden') === -1) {
            var recaptcha_theme = 'dark';

            grecaptcha.render('g-recaptcha', {
              'sitekey' : '6LcN8CITAAAAAL7vmmAmOoZrJ6uaFUjkc-20wnkc',
              'theme'   : recaptcha_theme,
              'size'    : 'compact'
            });
            window['recaptcha_rendered'] = true;
        }
    }

    function reload_image_captcha() {
        var img = $('captcha_img');
        img.src = img.src.replace(/\?.+$/, '')+'?random='+((new Date()).getTime());
        $('captcha').value='';
    }

    //
    _fajs.push(function(){
        $('login').activate();

        $('reload').observe('click', function(evt){
            evt.stop();
            reload_image_captcha();
        });


        //
        // recaptcha
        //
        $('captcha-switch').observe('click', function(evt) {
            //
            evt.stop();

            var recaptcha_container     = $$('.recaptcha-container').first();
            var image_captcha_container = $$('.image-captcha-container').first();
            var hidden_input            = image_captcha_container.down('input[type=hidden]');

            if(recaptcha_container.hasClassName('hidden')) {
                recaptcha_container.removeClassName('hidden');
                image_captcha_container.addClassName('hidden');
                hidden_input.value = 0;

                if(window['recaptcha_rendered'] === false) {
                    recaptcha_render();
                }
            } else {
                reload_image_captcha();
                recaptcha_container.addClassName('hidden');
                image_captcha_container.removeClassName('hidden');
                hidden_input.value = 1;
            }
        });
    });
</script>

<!-- recaptcha -->
